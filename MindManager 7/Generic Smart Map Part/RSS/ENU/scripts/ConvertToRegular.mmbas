Option Explicit
'#Uses "language.MMBas" ' language file placed by installation script
Sub Main
	Dim Selection As Selection
	Dim Topic As Topic
	Set Selection = ActiveDocument.Selection
	Set Topic = Selection.PrimaryTopic
	If (Topic Is Nothing) Then
		Exit Sub
	End If
    If Not(Topic.Attributes("http://schemas.mindjet.com/MindManager/GSMP/2003").HasAttribute("RssUrl")) Then
		Exit Sub
	End If
	Topic.Attributes("http://schemas.mindjet.com/MindManager/GSMP/2003").DeleteAll
End Sub
